   "source": [
    "# Count vowels\n",
    "def count_vowels(word):\n",
    "    c = 0\n",
    "    for x in word:\n",
    "        if x in 'aeiouAEIOU':\n",
    "            c += 1\n",
    "    return c"
   ]
